{
  "name": "snail-framework",
  "version": "0.1.0alpha1",
  "description": "I'm going to climb up to the top step by step and fly forward with the blades.",
  "main": "./dist/index.js",
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "cp build/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit",
    "prepush": "npm run lintall",
    "server": "nodemon server/index.js",
    "predev": "npm run precommit",
    "prestart": "npm run precommit",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --config ./build/webpack.config.js",
    "dev": "cross-env NODE_ENV=development node --max_old_space_size=4096 build/dev.js",
    "build": "cross-env NODE_ENV=production node --max_old_space_size=4096 build/build.js",
    "deploy": "cross-env NODE_ENV=production node ./node_modules/.bin/webpack --hide-modules --progress --config ./build/webpack.config.js",
    "lintall": "sh build/pre-commit lintall || true",
    "postlintall": "echo 'lintallall complete'",
    "jsdoc": "jsdoc ./src/*.js --readme ./README.md -t ./node_modules/ink-docstrap/template -c ./conf.json",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "author": {
    "name": "anchao",
    "email": "pvfhv@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pvfhv/snail-framework.git"
  },
  "keywords": [
    "webpack4",
    "sass",
    "compass",
    "react16.4.1",
    "redux",
    "immutable",
    "babel"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pvfhv/react-redux-router/issues"
  },
  "homepage": "https://github.com/pvfhv/react-redux-router",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
